# 阿里云MQ4IoT配置
MQTT_INSTANCE_ID=your_instance_id_here
MQTT_ENDPOINT=your_mqtt_endpoint_here
MQTT_ACCESS_KEY=your_access_key_here
MQTT_SECRET_KEY=your_secret_key_here
MQTT_GROUP_ID=GID_QUITTR
MQTT_DEVICE_ID=your_device_id_here
MQTT_PARENT_TOPIC=CHAT_OFFICIAL_GROUP
MQTT_QOS_LEVEL=0
# 连接配置
MQTT_USE_SSL=true
MQTT_SSL_PORT=8883
MQTT_TCP_PORT=1883
MQTT_CONNECTION_TIMEOUT=30
MQTT_KEEP_ALIVE=120
MQTT_AUTO_RECONNECT=true
MQTT_RECONNECT_DELAY=5000
MQTT_MAX_RECONNECT_ATTEMPTS=10

# 微信配置
WECHAT_APP_ID=your_wechat_app_id_here
WECHAT_APP_SECRET=your_wechat_app_secret_here

# 数据库配置
DDL_AUTO=validate
SHOW_SQL=false
HIBERNATE_SQL_LOG=false
